{
  "name": "activeinteractor.github.io",
  "version": "0.1.0",
  "description": "The ActiveInteractor Docs Site",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "scripts": {
    "build:css": "npm run _tw -- --jti -i ./lib/tailwind.css -o ./_sass/_tailwind.scss",
    "build:css:watch": "npm run build:css -- --watch",
    "_tw": "tailwind --config ./config/tailwind.config.js --postcss ./config/postcss.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeinteractor/activeinteractor.github.io.git"
  },
  "author": "Aaron Allen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/activeinteractor/activeinteractor.github.io/issues"
  },
  "homepage": "https://github.com/activeinteractor/activeinteractor.github.io#readme",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.5"
  }
}
